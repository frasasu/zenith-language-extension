{
  "Target Block": {
    "prefix": "target",
    "body": [
      "target ${1:name}:",
      "     key:\"${2:description}\"",
      "     dictionnary:",
      "               d1:\"${3:item 1}\"",
      "               d2:\"${4:item 2}\"",
      "               d3:\"${5:item 3}\"",
      "     ${6:laws}",
      "end_target"
    ],
    "description": "Create a new target block"
  },
  
  "Law Block": {
    "prefix": "law",
    "body": [
      "law ${1:aYYYY_MM_DD_HH_MM}:",
      "    start_date:${2:YYYY_MM_DD} at ${3:HH_MM}",
      "    period:${4:duration}",
      "    Event:",
      "          ${5:A}[d${6:1}]:\"${7:description}\"",
      "    GROUP:(${8:A} ${9:duration}^0)",
      "end_law"
    ],
    "description": "Create a law block"
  },
  
  "Simple Event": {
    "prefix": "event",
    "body": [
      "Event:",
      "      ${1:A}[d${2:1}]:\"${3:description}\""
    ],
    "description": "Add an event"
  },
  
  "Dictionary": {
    "prefix": "dict",
    "body": [
      "dictionnary:",
      "          d1:\"${1}\"",
      "          d2:\"${2}\"",
      "          d3:\"${3}\""
    ],
    "description": "Create dictionary"
  },
  
  "Multiple Events": {
    "prefix": "mevents",
    "body": [
      "Event:",
      "      A[d1]:\"${1}\"",
      "      B[d2]:\"${2}\"",
      "      C[d3]:\"${3}\"",
      "    GROUP:(A ${4:1.0}^0 - B ${5:1.0}^0 - C ${6:1.0}^0)"
    ],
    "description": "Multiple events"
  }
}